
<% produtos.forEach(function(produto) {%>
<div class="col-lg-4 col-md-6 mb-4">
    <div class="card h-100">
      <a href="/produtos/<%= produto._id %>"><img class="card-img-top" style="max-height:200px;" src="/uploads/<%= produto.img %>" alt=""></a>
      <div class="card-body">
        <h4 class="card-title">
          <a href="/produtos/<%= produtos._id %>"><%= produto.titulo %></a>
        </h4>
        <h5><%= produto.preco %></h5>
        <p class="card-text"><%= produto.descricao%></p>
      </div>
      <div class="card-footer">
        <small class="text-muted">&#9733; &#9733; &#9733; &#9733; &#9734;</small>
      </div>
    </div>
  </div>
  <% }) %>
